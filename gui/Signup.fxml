            <TextField fx:id="Age" layoutX="10.0" style="-fx-border-color: #0eeda2;" translateX="38.0" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="200.0" prefHeight="140.0" prefWidth="656.0">
         <children>
