               </opaqueInsets>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="email" prefHeight="25.0" prefWidth="153.0" promptText="username@gmail.com" style="-fx-border-color: #0eeda2;" translateX="55.0">
